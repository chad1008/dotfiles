{
	"workbench.colorTheme": "Solarized Dark",
	"telemetry.enableTelemetry": false,
	"html.format.preserveNewLines": false,
	"html.format.endWithNewline": true,
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.fontFamily": "MesloLGS NF",
	"editor.fontFamily": "Victor Mono, Menlo, Monaco, 'Courier New', monospace",
	"editor.fontWeight": 600,
	"editor.fontSize": 13,
	"terminal.integrated.fontSize": 13,
	"workbench.startupEditor": "none",
	"files.autoSave": "onFocusChange",
	"prettier.configPath": "",
	"editor.formatOnSave": true,
	"editor.snippetSuggestions": "top",
	"[javascript][json][typescript][typescriptreact][css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true
	},
	"[markdown]": {
		"editor.formatOnSave": false
	},
	"explorer.confirmDelete": false,
	"editor.bracketPairColorization.enabled": true,
	"explorer.confirmDragAndDrop": false,
	"window.newWindowDimensions": "offset",
	"workbench.colorCustomizations": {
		"terminal.foreground": "#839496",
		"terminal.selectionBackground": "#073642",
		"terminal.ansiBlack": "#073642",
		"terminal.ansiBrightBlack": "#667e8f",
		"terminal.ansiRed": "#dc322f",
		"terminal.ansiBrightRed": "#cb4b16",
		"terminal.ansiGreen": "#008b1d",
		"terminal.ansiBrightGreen": "#389900",
		"terminal.ansiYellow": "#b58900",
		"terminal.ansiBrightYellow": "#cecb02",
		"terminal.ansiBlue": "#268bd2",
		"terminal.ansiBrightBlue": "#2a6fd9",
		"terminal.ansiMagenta": "#8F4673",
		"terminal.ansiBrightMagenta": "#d33682",
		"terminal.ansiCyan": "#28958c",
		"terminal.ansiBrightCyan": "#19b7ab",
		"terminal.ansiWhite": "#eee8d5",
		"terminal.ansiBrightWhite": "#fdf6e3",
		"editorBracketHighlight.foreground1": "#008ad6",
		"editorBracketHighlight.foreground2": "#1da501",
		"editorBracketHighlight.foreground3": "#a19f00",
		"editorBracketHighlight.foreground4": "#bd8800",
		"editorBracketHighlight.foreground5": "#db3f00",
		"editorBracketHighlight.foreground6": "#993bf1"
	},
	"editor.semanticTokenColorCustomizations": {
		"[Solarized Dark]": {
			"enabled": true,
			"rules": {
				"method": {
					"foreground": "#6ea9c9"
				},
				"method.declaration": {
					"bold": true,
					"foreground": "#008ad6"
				},
				"parameter": {
					"foreground": "#24a75f"
				}
			}
		}
	},
	"files.watcherExclude": {
		"**/home/wpcom/public_html/**": true
	},
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"editor.insertSpaces": false,
	"workbench.iconTheme": "material-icon-theme",
	"editor.stickyScroll.enabled": true,
	"terminal.integrated.minimumContrastRatio": 1,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"vscode-pets.petType": "dog",
	"vscode-pets.petSize": "medium",
	"vscode-pets.position": "explorer",
	"vsicons.dontShowNewVersionMessage": true,
	"editor.inlineSuggest.enabled": true,
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"editor.accessibilitySupport": "off",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.rulers": [
		{
			"column": 80,
			"color": "#008bd62a"
		}
	],
	"totalTypeScript.hideAllTips": false,
	"totalTypeScript.hideBasicTips": false,
	"totalTypeScript.hiddenTips": [
		"type-alias-declaration",
		"ts-object-type",
		"basic-types",
		"optional-object-property",
		"union-type",
		"literal-type",
		"passing-generics-to-types",
		"typing-function-parameters",
		"pick-utility-type",
		"array-type",
		"variable-type-annotation",
		"nonnullable-utility-type",
		"null-keyword",
		"typeof",
		"omit-utility-type",
		"any-type",
		"function-return-type",
		"as-assertion",
		"type-alias-with-generics",
		"tuple-type",
		"undefined-keyword",
		"never-keyword",
		"type-predicate",
		"in-operator-narrowing",
		"conditional-type",
		"generic-slots-in-functions",
		"returntype-utility-type"
	],
	"diffEditor.renderSideBySide": false,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": true
	}
}
